<% provide(:title, "Update post") %> 
<h1>Update post</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@post) do |f| %>
   	  <%= f.label :title %>
      <%= f.text_field :password %>

      <%= f.label :content %>
      <%= f.text_area :content %>

      <%= f.submit "Save changes", class: "btn btn-large btn-primary" %>
    <% end %>
  </div>
</div>
